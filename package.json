{
  "name": "tms-scrape",
  "version": "0.4.22",
  "description": "scrape a single dynamic web page with JavaScript or from command line",
  "main": "doscrape.js",
  "type": "module",
  "bin": {
    "scrape": "./index.js"
  },
  "scripts": {
    "scrape": "./index.js",
    "test1": "./index.js --url https://terrymorse.com/packages/index.html --config test1-config.json --dest test1",
    "test2": "./index.js --url https://www.iana.org/domains/reserved --dest test2",
    "test3": "./index.js --url  https://www.washingtonpost.com/world/2022/04/13/russia-ukraine-war-news-live-updates/ --dest test3",
    "test4": "./index.js --url  https://www.iana.org/domains/reserved --config test4-config.json --dest test4",
    "test5": "./index.js --url  https://www.washingtonpost.com/health/2022/04/18/long-covid-medical-care-challenge/ --config test5-config.json --dest test5",
    "test6": "./index.js --url  https://www.bloomberg.com/news/articles/2022-05-25/biden-demands-us-stand-up-to-gun-makers-after-texas-massacre?srnd=premium --config test6-config.json --dest test6"
  },
  "author": "Terry Morse",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terrymorse58/tms-scrape"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "jsdom": "^19.0.0",
    "website-scraper": "^5.0.0",
    "website-scraper-puppeteer": "^1.0.0"
  }
}
